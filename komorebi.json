{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.29/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.yaml",
  "minimum_window_width": 0,
  "minimum_window_height": 0,
  "resize_delta": 50,
  "window_container_behaviour": "Create",
  "cross_monitor_move_behaviour": "Insert",
  "cross_boundary_behaviour": "Monitor",
  "unmanaged_window_operation_behaviour": "NoOp",
  "mouse_follows_focus": false,
  "border_width": 0,
  "border_offset": 0,
  "border": false,
  "border_colours": {
    "single": "#89b4fa", //#89b4fa
    "stack": "#a6e3a1",
    "monocle": "#f38ba8",
    "unfocused": "#10151D"
  },
  "border_style": "Rounded",
  "border_z_order": "Bottom",
  "border_implementation": "Windows",
  "transparency": false,
  "transparency_alpha": 220,
  "default_workspace_padding": 3,
  "default_container_padding": 3,
  "animation": {
    "enabled": true,
    "style": "EaseInOutExpo",
