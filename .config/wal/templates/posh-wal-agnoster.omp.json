{{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {{
      "alignment": "left",
      "segments": [
        {{
          "background": "{color11}",
          "foreground": "{color0}",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        }},
        {{
          "background": "{color15}",
          "foreground": "{color0}",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{{{ .UserName }}}}@{{{{ .HostName }}}} ",
          "type": "session"
        }},
        {{
          "background": "{color12}",
          "foreground": "{color0}",
          "powerline_symbol": "\ue0b0",
          "properties": {{
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\uf7db",
            "style": "agnoster"
          }},
          "style": "powerline",
          "template": " {{{{ .Path }}}} ",
          "type": "path"
        }},
        {{
          "background": "{color10}",
          "foreground": "{color4}",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{{{ .HEAD }}}} ",
          "type": "git"
        }},
        {{
          "background": "{color5}",
          "foreground": "{color0}",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{{{ if .Error }}}}{{{{ .Error }}}}{{{{ else }}}}{{{{ if .Venv }}}}{{{{ .Venv }}}} {{{{ end }}}}{{{{ .Full }}}}{{{{ end }}}} ",
          "type": "python"
        }},
        {{
          "background": "{color9}",
          "foreground": "{color15}",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{{{ .Meaning }}}} ",
          "type": "exit"
        }}
      ],
      "type": "prompt"
    }}
  ],
  "final_space": true,
  "version": 2
}}